{
  "name": "@oxog/routely",
  "version": "1.0.0",
  "description": "The router that respects your bundle. Type-safe, extensible, tiny.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins.d.ts",
        "default": "./dist/plugins.js"
      },
      "require": {
        "types": "./dist/plugins.d.cts",
        "default": "./dist/plugins.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@oxog/routely-core": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "jsdom": "^23.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "router",
    "react-router",
    "type-safe",
    "typescript",
    "routing",
    "spa",
    "ssr",
    "plugin",
    "lightweight",
    "minimal",
    "navigation",
    "tanstack-alternative"
  ],
  "author": "Ersin Ko√ß",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersinkoc/routely.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/ersinkoc/routely/issues"
  },
  "homepage": "https://routely.oxog.dev"
}
