{
  "name": "@oxog/routely-core",
  "version": "1.0.0",
  "description": "Framework-agnostic routing kernel for Routely",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "router",
    "routing",
    "navigation",
    "framework-agnostic",
    "type-safe",
    "typescript",
    "zero-dependencies"
  ],
  "author": "Ersin Ko√ß",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersinkoc/routely.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/ersinkoc/routely/issues"
  },
  "homepage": "https://routely.oxog.dev",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
