{
  "name": "@oxog/routely-plugin-scroll",
  "version": "1.0.0",
  "description": "Scroll behavior plugin for Routely",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@oxog/routely": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "happy-dom": "^13.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "router",
    "scroll",
    "scroll-restoration",
    "routely-plugin"
  ],
  "author": "Ersin Ko√ß",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersinkoc/routely.git",
    "directory": "plugins/scroll"
  },
  "bugs": {
    "url": "https://github.com/ersinkoc/routely/issues"
  },
  "homepage": "https://routely.oxog.dev/plugins/scroll"
}
