{
  "name": "@oxog/routely-plugin-devtools",
  "version": "1.0.0",
  "description": "Developer tools plugin for Routely router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "routely",
    "router",
    "devtools",
    "developer"
  ],
  "author": "Oxog",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "@oxog/routely-core": "workspace:*",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@oxog/routely-core": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.0",
    "happy-dom": "^12.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
